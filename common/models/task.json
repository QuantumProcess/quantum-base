{
  "name": "Task",
  "plural": "Tasks",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "priority": {
      "type": "string"
    },
    "state": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "assigned": {
      "type": "hasMany",
      "model": "user"
    },
    "organization": {
      "type": "belongsTo",
      "model": "Organization",
      "foreignKey": "organizationId"
    },
    "area": {
      "type": "belongsTo",
      "model": "Area",
      "foreignKey": "areaId"
    },
    "project": {
      "type": "belongsTo",
      "model": "Project",
      "foreignKey": "projectId"
    },
    "subtask": {
      "type": "hasMany",
      "model": "Subtask",
      "foreignKey": "taskId"
    },
    "type": {
      "type": "hasOne",
      "model": "TaskType",
      "foreignKey": "typeId"
    }
  },
  "acls": [],
  "methods": []
}
